{
  "name": "ema",
  "version": "2.6.0",
  "main": "electron/dist/electron-main.js",
  "description": "POS integracion con SAP B1",
  "author": "Clavis Consultores S.A.",
  "scripts": {
    "ng": "ng",
    "start": "tsc --p electron && electron .",
    "debug": "electron .",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "tsc --p electron && ng build --prod && electron .",
    "package-win": "electron-packager . --overwrite",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "ClvsPos",
    "copyright": "Copyright Â© 2020 Clavis Consultores S.A.",
    "win": {
      "icon": "build/assets/clavisco.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "include": "build/assets/installer.nsh",
      "oneClick": false,
      "displayLanguageSelector": true,
      "allowToChangeInstallationDirectory": true
    },
    "asar": false,
    "directories": {
      "output": "build/installer/"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "@swimlane/ngx-datatable": "^15.0.2",
    "@types/electron": "^1.6.10",
    "angular-font-awesome": "^3.1.2",
    "bootstrap": "^4.1.3",
    "chart.js": "^2.8.0",
    "core-js": "^2.5.4",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "ng-block-ui": "^2.0.0",
    "ng-ip-address": "^1.1.13",
    "ng2-charts": "^2.3.0",
    "ng2-pdf-viewer": "^5.2.2",
    "ngx-electron": "^2.1.1",
    "node-sass": "^4.14.1",
    "pdf-to-printer": "^1.4.0",
    "popper.js": "^1.14.5",
    "print-js": "^1.0.61",
    "rxjs": "~6.3.3",
    "sweetalert2": "^7.33.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "~7.0.5",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "electron": "^9.1.1",
    "electron-builder": "^22.7.0",
    "electron-packager": "^15.0.0",
    "electron-rebuild": "^1.8.6",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
